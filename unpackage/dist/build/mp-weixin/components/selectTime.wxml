<view class="selectTime"><view class="mark"></view><view class="selectContent"><text class="title">请选择可预订日期</text><scroll-view class="select-wrapper" style="{{('height:'+heights+'px')}}" scroll-y><view class="selectWrapper"><block wx:for="{{dataList}}" wx:for-item="dt" wx:for-index="index" wx:key="index"><view class="item"><view class="itemWeek"><text>日</text><text>一</text><text>二</text><text>三</text><text>四</text><text>五</text><text>六</text></view><view class="date">{{dt.dataTime}}</view><view class="dateList"><block wx:for="{{dt.nodata}}" wx:for-item="noData" wx:for-index="i"><view><text></text></view></block><block wx:for="{{dt.data}}" wx:for-item="data" wx:for-index="d" wx:key="d"><view data-event-opts="{{[['tap',[['selectDate',['$0','$1'],[[['dataList','',index],['data','',d]],[['dataList','',index,'toDay']]]]]]]}}" class="{{[(data.data<=dt.toDay)?'remove':'',(data.isSelect&&!data.isLock)?'active':'',(data.isLock)?'lock':'']}}" bindtap="__e"><text>{{data.data}}</text></view></block></view></view></block></view></scroll-view></view><view class="timeBtn"><text data-event-opts="{{[['tap',[['selectTime',['$event']]]]]}}" bindtap="__e">确定</text></view></view>